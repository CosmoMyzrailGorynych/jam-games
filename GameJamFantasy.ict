  "rooms": [
    {
      "name": "InGame",
      "oncreate": "this.music = ct.sound.spawn('HackneyLives', {\n    loop: true,\n    volume: 0.75\n});\n\n// ct.x.imageSmoothingEnabled = ct.x.mozImageSmoothingEnabled = false;\n\nthis.scrollFactor = 0.2;\nthis.overheat = 0;\nthis.spawnTimer = -1;\nthis.bossMode = 0;\n\nthis.y = 32;\n\nthis.miniStage = 0;\nthis.macroStage = 0;\n\nthis.bossHealth = 1000;\n\n/*\nvar width = 20,\n    height = 12,\n    grid = 32,\n    cx = this.width / 2,\n    cy = this.height / 2;\nfor (var i = 1; i < width; i++) {\n    ct.types.copy('WallT', cx + grid*(i - width/2), cy - grid*height / 2 - 32);\n    ct.types.copy('WallB', cx + grid*(i - width/2), cy + grid*height / 2);\n}\nfor (var i = 1; i < height; i++) {\n    ct.types.copy('WallL', cx - grid*width / 2, cy + grid*(i - height/2));\n    ct.types.copy('WallR', cx + grid*width / 2, cy + grid*(i - height/2));\n}\nct.types.copy('WallTL', cx - grid*width/2, cy - grid*height/2 - 32);\nct.types.copy('WallBL', cx - grid*width/2, cy + grid*height/2);\nct.types.copy('WallTR', cx + grid*width/2, cy - grid*height/2 - 32);\nct.types.copy('WallBR', cx + grid*width/2, cy + grid*height/2);\n*/\n\nthis.themes = [\n    ['Iceberg', 'IceSnake'],\n    ['Skeleton'/*, 'Skeleton_Cool'*/, 'DeadlyBear'],\n    ['FireWisp', 'FireSkull'],\n    ['Cloud', 'Bat']\n];\n\nthis.toSpawn = [];\nthis.activeMobs = [];\nmakeUpEnemies(this.themes, this.toSpawn);\n\nthis.deadIceCounter = 5;\nthis.deadFireCounter = 5;\nthis.deadEarthCounter = 5;\nthis.deadWindCounter = 5;",
      "onstep": "var mx = ct.mouse.x - this.x,\n    my = ct.mouse.y - this.y,\n    hero = ct.types.list['TheWizard'][0] || false;;\nif (hero) {\n    var power = ct.u.pdc(hero.x, hero.y, ct.mouse.x, ct.mouse.y) / 500,\n        targetX = hero.x - this.width / 2 + (mx - hero.x) * this.scrollFactor * power,\n        targetY = hero.y - this.height / 2 + (my - hero.y) * this.scrollFactor * power;\n    this.x = Math.floor(targetX * 0.25 + this.x * 0.75);\n    this.y = Math.floor(targetY * 0.25 + this.y * 0.75);\n    \n    if (this.startGame) {\n        this.spawnTimer --;\n        if (this.spawnTimer <= 0) {\n            if (this.toSpawn.length) {\n                var toSpawn = this.toSpawn.pop(),\n                    summoner = ct.types.copy('Summoner', ct.random(this.width), ct.random(this.height / 2));\n                summoner.toSummon = toSpawn;\n                while (ct.u.pdc(hero.x, hero.y, summoner.x, summoner.y) < 100 || ct.place.occupied(summoner, summoner.x, summoner.y, 'Solid')) {\n                    summoner.x = ct.random(this.width);\n                    summoner.y = 20 + ct.random(this.height / 2);\n                }\n                this.spawnTimer = 10;   \n            } else {\n                if (!this.activeMobs.length) {\n                    makeUpEnemies(this.themes, this.toSpawn);\n                    this.miniStage ++;\n                    if (this.miniStage > 1) {\n                        var x = 100 + Math.floor(ct.random.range(0, this.width - 200)),\n                        y = 75 + Math.floor(ct.random.range(0, this.height - 150));\n                        ct.types.copy('BossAppear', x, y);\n                        this.miniStage = 0;\n                        this.macroStage ++;\n                        if (this.macroStage == 2) {\n                            ct.sound.load('VienneseWorlds_Mini');\n                        }\n                        this.spawnTimer = 60 * 10; \n                    } else {\n                        this.spawnTimer = 60 * 1; \n                    }\n                    if (!this.lastStaff || this.lastStaff.kill) {\n                        this.lastStaff = ct.types.copy(ct.random.dice('RockStaff', /*'LianeStaff',*/ 'WindStaff', 'FireStaff', /*'BeamStaff',*/ 'IcicleStaff'), this.width / 2, this.height / 2);\n                    }\n                }\n            }\n        }\n    }\n}\n",
      "ondraw": "/* Overheat */\n\nvar hero = ct.types.list['TheWizard'][0] || false;\nif (hero) {\n    this.overheat = this.overheat * 0.75 + hero.overheat * 0.25;\n    \n    var startx = Math.floor(this.width / 2 - 65),\n        starty = this.height - 53;\n    \n    ct.styles.set('OverchargeBase');\n    ct.draw.fix(startx - 3, starty - 3);\n    ct.draw.rectangle(this.x, this.y, 106, 18);\n    \n    ct.x.shadowColor = 'transparent';\n    ct.draw.rectangle(this.x, this.y, 106, 18, true);\n    ct.draw.unfix();\n    ct.styles.reset();\n    \n    ct.styles.set('OverchargeBar_' + hero.form);\n    ct.draw.fix(startx, starty);\n    ct.draw.rectangle(this.x, this.y, Math.min(this.overheat, 100), 12);\n    \n    ct.draw.img('Skull', 0, this.x + 119, this.y + 6);\n    \n    ct.draw.unfix();\n    \n    ct.styles.reset();   \n}\n\n/* Health */\n\nif (hero) {\n    var startx = Math.floor(this.width / 2 - 10 * 5) + this.x,\n        starty = this.height - 22 + this.y;\n    for (var i = 0; i < hero.hp - (hero.hp%2); i += 2) {\n        ct.draw.img('Heart', 0, startx + 5*i, starty);\n    }\n    if (hero.hp % 2 === 1) {\n        ct.draw.img('Heart', 1, startx + 5*(i), starty);\n        i+=2;\n    }\n    for (; i < 20; i += 2) {\n        ct.draw.img('Heart', 2, startx + 5*i, starty);\n    }\n    \n    /**Boss Health **/\n    var startx = Math.floor(this.width / 2 - 200) + this.x,\n        starty = 10 + this.y;\n    ct.draw.fix(startx, starty);\n    ct.styles.set('OverchargeBase');\n    ct.draw.rectangle(0, 0, 400, 8);\n    ct.styles.reset();\n    ct.x.fillStyle = '#f60000';\n    ct.draw.rectangle(2, 2, this.bossHealth / 1000 * 396, 4);\n    ct.draw.unfix();\n}\n",
