  "scripts": [
    {
      "name": "catchOverheatStep",
      "code": "var catchOverheatStep = function(prev, next) {\n    if (prev < 25 && next >= 25) {\n        ct.sound.spawn('Overcharge1');\n    } else if (prev < 50 && next >= 50) {\n        ct.sound.spawn('Overcharge2');\n    } else if (prev < 75 && next >= 75) {\n        ct.sound.spawn('Overcharge3');\n    } else if (prev < 95 && next >= 95) {\n        ct.sound.spawn('Overcharge4');\n    }\n}\n"
    },
    {
      "name": "enemyBehaviour",
      "code": "var enemyBehaviourOld = function (copy) {\n    if (copy.freezeTimer && copy.freezeTimer > 0) {\n        copy.freezeTimer --;\n        copy.spd = 0;\n        return;\n    }\n    if (copy.poisionTimer && copy.poisionTimer > 0) {\n        copy.poisionTimer --;\n    }\n    if (copy.electrocutedTimer && copy.electrocutedTimer > 0) {\n        copy.electrocutedTimer --;\n    }\n    var hero = ct.types.list['TheWizard'][0];\n    if (!copy.behaviourTimer) {\n        copy.behaviourTimer = Math.floor(ct.random.range(20, 60));\n        if (copy.burning) {\n            copy.hp -= 1;\n            if (copy.hp <= 0) {\n                copy.kill = true;\n                return;\n            }\n        }\n        if (hero) {\n            var dist = ct.u.pdc(copy.x, copy.y, hero.x, hero.y),\n                dir = ct.u.pdn(copy.x, copy.y, hero.x, hero.y) + ct.random.range(-60, 60);\n            if (dist < 80) {\n                copy.xx = hero.x + ct.random.range(-40, 40);\n                copy.yy = hero.y + ct.random.range(-40, 40);\n                if (dist <= 40) {\n                    damageHero(hero, copy.damage);\n                }\n            } else {\n                copy.xx = copy.x + ct.u.ldx(200, dir);\n                copy.yy = copy.x + ct.u.ldy(200, dir);\n                if (dist <= 10) {\n                    copy.spd = 0;\n                } else {\n                    copy.spd = copy.speed;\n                    if (copy.poisionTimer && copy.poisionTimer > 0) {\n                        copy.spd /= 2;\n                    }\n                }\n            }\n            copy.xx = Math.min(Math.max(32, copy.xx), ct.room.width - 32);\n            copy.yy = Math.min(Math.max(32, copy.yy), ct.room.height - 32);\n            copy.dir = ct.u.pdn(copy.x, copy.y, copy.xx, copy.yy);\n        } else {\n            copy.spd = 0;\n        }\n    } else {\n        copy.behaviourTimer --;\n    }\n}\n\nvar enemyBehaviour = function (copy) {\n    copy.spd = 0;\n    var spd = copy.speed;\n    if (!copy.effectsTick) {\n        copy.effectsTick = 1;\n    } else {\n        copy.effectsTick ++;\n    }\n    if (copy.freezeTimer && copy.freezeTimer > 0) {\n        copy.freezeTimer --;\n        spd = 0;\n        return;\n    }\n    if (copy.poisionTimer && copy.poisionTimer > 0) {\n        copy.poisionTimer --;\n        spd /= 2;\n    }\n    var hero = ct.types.list['TheWizard'][0];\n    if (!copy.behaviourTimer) {\n        copy.behaviourTimer = Math.floor(ct.random.range(20, 60));\n        if (copy.burning) {\n            copy.hp -= 1;\n            if (copy.hp <= 0) {\n                copy.kill = true;\n                return;\n            }\n        }\n        if (hero) {\n            var dist = ct.u.pdc(copy.x, copy.y, hero.x, hero.y),\n                dir = ct.u.pdn(copy.x, copy.y, hero.x, hero.y) + ct.random.range(-60, 60);\n            if (dist < 80) {\n                copy.xx = hero.x + ct.random.range(-40, 40);\n                copy.yy = hero.y + ct.random.range(-40, 40);\n                if (dist <= 40) {\n                    damageHero(hero, copy.damage);\n                }\n            } else {\n                copy.xx = copy.x + ct.u.ldx(200, dir);\n                copy.yy = copy.x + ct.u.ldy(200, dir);\n                if (dist <= 10) {\n                    spd = 0;\n                }\n            }\n            copy.dir = ct.u.pdn(copy.x, copy.y, copy.xx, copy.yy);\n        } else {\n            spd = 0;\n        }\n    } else {\n        copy.behaviourTimer --;\n    }\n    if (spd !== 0) {\n        ct.place.go(copy, copy.xx, copy.yy, spd, 'Solid');\n    }\n}"
    },
    {
      "name": "enemyDraw",
      "code": "var enemyDraw = function (copy) {\n    var cx = Math.floor(copy.x),\n        cy = Math.floor(copy.y);\n    if (copy.dir > 90 && copy.dir <= 270) {\n        copy.tx = 1;\n    } else {\n        copy.tx = -1;\n    }\n    ct.draw.img('Shadow', 0, cx, cy);\n    ct.draw(copy, cx, cy);\n    if (copy.burning) {\n        ct.draw.img('BurnEffect', Math.floor((copy.effectsTick / 5) % 5), cx, cy);\n    }\n    if (copy.electrocutedTimer) {\n        ct.draw.img('Electrocuted', Math.floor((copy.effectsTick / 5) % 8), cx, cy);\n    }\n    if (copy.freezeTimer) {\n        ct.draw.img('IceEffect', 4, cx, cy);\n    }\n}"
    },
    {
      "name": "damageHero",
      "code": "var damageHero = function(hero, dmg) {\n    hero.hp -= dmg;\n    ct.sound.spawn(ct.random.dice('Hurt1', 'Hurt2', 'Hurt3'));\n    if (hero.hp <= 0 && !hero.kill) {\n        hero.kill = true;\n        ct.room.startGame = false;\n        ct.types.copy('FadeOut', 0, 0);\n        setTimeout(function() {\n            var reason = ct.types.copy('DeathScreen', ct.room.x + ct.room.width / 2, ct.room.y + ct.room.height / 2);\n            reason.frame = ct.random.dice(0, 1, 2);\n        }, 1000);\n        ct.sound.fade('HackneyLives', 0, 1000, ct.room.music);\n    }\n}"
    },
    {
      "name": "makeUpEnemies",
      "code": "var makeUpEnemies = function(themes, toSpawn) {\n    var theme1 = themes[~~(Math.random() * themes.length)],\n        theme2 = themes[~~(Math.random() * themes.length)],\n        l1 = ct.random.range(2, 3 + Math.min(ct.room.macroStage, 5)),\n        l2 = ct.random.range(2, 3 + Math.min(ct.room.macroStage, 5));\n    for (var i = 1; i < l1; i++) {\n        toSpawn.push(theme1[~~(Math.random() * theme1.length)]);\n    }\n    for (var i = 1; i < l2; i++) {\n        toSpawn.push(theme2[~~(Math.random() * theme2.length)]);\n    }\n}"
    },
    {
      "name": "enemyDeath",
      "code": "var enemyDeath = function (copy, power) {\n    var c = 'dead' + power + 'Counter';\n    ct.room[c]++;\n    var cx = Math.floor(copy.x),\n        cy = Math.floor(copy.y);\n    if (ct.room[c] === 10) {\n        if (power === 'Earth') {\n            ct.types.copy(ct.random.dice('RockStaff'), cx, cy);   \n        } else if (power === 'Ice') {\n            ct.types.copy(ct.random.dice('IcicleStaff'), cx, cy);   \n        } else if (power === 'Fire') {\n            ct.types.copy(ct.random.dice('FireStaff'), cx, cy);   \n        } else if (power === 'Wind') {\n            ct.types.copy(ct.random.dice('WindStaff'), cx, cy);   \n        }\n        ct.room[c] = 0;\n    }\n    ct.room.activeMobs.splice(ct.room.activeMobs.indexOf(copy), 1);\n    ct.sound.spawn('EnemyDeath');\n    ct.types.copy('FX_EnemyDeath', cx, cy);\n}"
    },
    {
      "name": "bossDamageHandler",
      "code": "var bossDamageHandler = function (boss) {\n    if (!boss.hp) {\n        boss.hp = 1000;\n        boss.hpPrev = boss.hp;\n    }\n    if (boss.hpPrev !== boss.hp) {\n        ct.room.bossHealth -= boss.hpPrev - boss.hp;\n    }\n    boss.hpPrev = boss.hp;\n    if (ct.room.bossHealth <= 0) {\n        boss.kill = true;\n        ct.room.bossHealth = 0;\n        ct.types.each(function () {\n            if (this.ctype === 'Enemy' || this.ctype === 'EnemyBullet' || this.type === 'Summoner') {\n                this.kill = true;\n            }\n        });\n        ct.types.copy('BossDeath', boss.x, boss.y);\n    }\n};"
    }
  ],
