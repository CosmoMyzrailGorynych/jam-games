    {
      "name": "Fireball",
      "depth": 0,
      "oncreate": "this.spd = 7;\nthis.imgspd = 0.2;\nthis.transform = true;\n\nct.sound.spawn('FireballLaunch');",
      "onstep": "ct.types.move(this);\n\nif (ct.place.occupied(this, this.x, this.y, 'Solid') || ct.place.occupied(this, this.x, this.y, 'Enemy')) {\n    this.kill = true;\n}\n\nthis.depth = - this.y + 10;\n",
      "ondraw": "this.tr = this.dir;\nct.draw(this);\nif (this.frame >= 5) {\n    this.frame = 2;\n}",
      "ondestroy": "ct.sound.spawn('FireballExplosion');\nct.types.copy('FX_Explosion', this.x, this.y);\n\nvar me = this;\nct.types.each(function () {\n    if (this.ctype !== 'Enemy') {\n        return;\n    } else if (ct.u.pdc(this.x, this.y, me.x, me.y) < 64) {\n        this.hp -= 7;\n        this.freezeTimer = 0;\n        if (this.form !== 'Fire') {\n            this.burning = true;\n        }\n        if (this.hp < 0) {\n            this.kill = true;\n        }\n        if (this.electrocutedTimer && this.type.indexOf('Boss') === -1) {\n            this.hp -= 20;\n            ct.types.copy('FX_Explosion', ~~(this.x), ~~(this.y - 16));\n        }\n    }\n});",
      "uid": "81ff22c8-8da6-46bc-8a5e-38f9517d4604",
      "graph": "bf5b308a-ea02-4f59-9725-41210de23906"
    },
    {
      "name": "FireStaff",
      "depth": 0,
      "oncreate": "this.depth = -1000;\nthis.imgspd = 0.2;\nvar FX = ct.types.copy('FX_StaffSparkles', this.x, this.y);\nFX.parent = this;",
      "onstep": "var hero = ct.place.meet(this, this.x, this.y, 'TheWizard');\nif (hero) {\n    this.kill = true;\n    hero.projectile = 'Fireball';\n    hero.overheat = 0;\n    hero.shootPeriod = 45;\n    hero.overheatCost = 10;\n    if (hero.form !== 'Fire') {\n        ct.sound.spawn('Transform');\n    }\n    hero.form = 'Fire';\n    hero.staff = 'Fire';\n}",
      "ondraw": "ct.draw(this);",
      "ondestroy": "",
      "uid": "d969c05e-f4e9-4a7a-8122-4a46ed2a87b4",
      "graph": "bde07613-dd83-4b31-8db7-89f4289643ce"
    },
    {
      "name": "Icicle",
      "depth": 0,
      "oncreate": "this.spd = 10;\nthis.transform = true;\nthis.dir = ct.random.range(-10, 10);\nct.sound.spawn('IcicleLaunch');",
      "onstep": "if (ct.place.occupied(this, this.x, this.y, 'Solid')) {\n    this.kill = true;\n} else {\n    var enemy = ct.place.occupied(this, this.x, this.y, 'Enemy');\n    if (enemy) {\n        this.kill = true;\n        enemy.hp -= 3;\n        if (enemy.form !== 'Ice') {\n            enemy.freezeTimer = 60 * 3;\n        }\n        enemy.burning = false;\n        if (enemy.hp <= 0) {\n            enemy.kill = true;\n        }\n    } else {\n        this.depth = - this.y + 10;\n        ct.types.move(this);\n        this.tr = this.dir;\n    }\n}\n",
      "ondraw": "ct.draw(this);",
      "ondestroy": "ct.sound.spawn('IceHit');\nct.types.copy('FX_Impact', this.x, this.y);",
      "uid": "30680475-113c-4e26-9099-5837d5c59852",
